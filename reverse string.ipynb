    "def reverse(str): \n",
    "  s = \"\" \n",
    "  for i in str: \n",
    "    s = i + s\n",
    "  return s\n",
    "str=input(\"enter a string :\")\n",
    "print(\"The original string  is :\",str) \n",
    "print(\"after reversing string is :\",reverse(str))"
   ]
  }
